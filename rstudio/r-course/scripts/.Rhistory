scale.many = function(dataframe, cols) {
names <- names(dataframe)
for (col in cols) {
name <- paste(names[col], "z", sep = ".")
dataframe[name] <- scale(dataframe[,col])
}
cat(paste("Hemos normalizado ", length(cols), " variable(s)"))
dataframe
}
